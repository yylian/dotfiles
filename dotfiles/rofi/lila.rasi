* {
	bg: #0f111b;
	accent: #ecf0c1;
	fg: #818596;
	background-color: @bg;
	text-color: @fg;
}

window {
	padding: 7px;
	border: 1px;
	border-color: @accent;
    width:  400px;
}

prompt, entry {
	text-color: @accent;
}

element {
	border-radius: 0px;
	padding: 3px;
}

element selected {
	background-color: @accent;
	text-color: @bg;
}

element-text selected {
    background-color: @accent;
	text-color: @bg;
}